{namespace components}

/**
 * Icon 路 12x12
 */
{template .icon12x12}
  {let $id: 'icon12x12' /}
  {let $title: 'Icon 路 12x12' /}
  {let $description: 'These are icons built in a 12x12 canvas. When scaling them, always follow the right proportion, for example: 24x24, 36x36, 48x48, etc.' /}
  {let $html kind="html"}
{literal}<span class="icon-12-clock"></span>
<span class="icon-12-pin"></span>
<span class="icon-12-ballon-text"></span>
<span class="icon-12-camera"></span>
<span class="icon-12-link"></span>
<span class="icon-12-heart-outline"></span>
<span class="icon-12-star"></span>
<span class="icon-12-post"></span>
<span class="icon-12-close-long"></span>
<span class="icon-12-person"></span>
<span class="icon-12-building-small"></span>
<span class="icon-12-people"></span>
<span class="icon-12-hash"></span>
<span class="icon-12-close-short"></span>
<span class="icon-12-heart-inline"></span>
<span class="icon-12-arrow-left-long"></span>
<span class="icon-12-arrow-right-long"></span>
<span class="icon-12-plus"></span>
<span class="icon-12-person-card-3"></span>
<span class="icon-12-person-card-2"></span>
<span class="icon-12-building-big"></span>
<span class="icon-12-groups"></span>
<span class="icon-12-persons"></span>
<span class="icon-12-balloon-topic"></span>
<span class="icon-12-balloon-comments"></span>
<span class="icon-12-balloon-comments-inline"></span>
<span class="icon-12-heart-full"></span>
<span class="icon-12-arrow-down-short"></span>
<span class="icon-12-arrow-up-short"></span>
<span class="icon-12-arrow-right-rod"></span>
<span class="icon-12-arrow-left-rod"></span>
<span class="icon-12-arrows"></span>
<span class="icon-12-browser"></span>
<span class="icon-12-trash"></span>{/literal}
  {/let}
  {let $snippet kind="text"}{$html}{/let}
  {call static.cell}
    {param id: $id /}
    {param title: $title /}
    {param description: $description /}
    {param snippet: $snippet /}
    {param html: $html /}
  {/call}
{/template}

